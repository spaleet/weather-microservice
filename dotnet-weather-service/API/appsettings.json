{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "WeatherSettings": {
    "ApiKey": "<API KEY>",
    "BaseUrl": "https://api.openweathermap.org/data/2.5"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information",
        "System.Net.Http.HttpClient": "Error"
      },
      "Using": [ "Serilog.Sinks.Console" ]
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  }
}
